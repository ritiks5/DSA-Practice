    "def kadane(nums):\n",
    "    maxsum=-10**6\n",
    "\n",
    "    currsum=0\n",
    "    i=0\n",
    "    while i<len(nums):\n",
    "        currsum+=nums[i]\n",
    "        if currsum<0:\n",
    "            currsum=0\n",
    "        else:\n",
    "            maxsum=max(maxsum,currsum)\n",
    "        i+=1\n",
    "    return maxsum\n",
    "\n",
    "nums = [5,4,-1,7,8]\n",
    "sum = kadane(nums)\n",
    "print(sum)"
   ]
  }
