    "def multiply(a,b):\n",
    "    if a==0 or b==0:\n",
    "        return 0\n",
    "    \n",
    "    if b==1:\n",
    "        return a\n",
    "    \n",
    "    return a + multiply(a,b-1)\n",
    "\n",
    "print(multiply(6,9))"
   ]
