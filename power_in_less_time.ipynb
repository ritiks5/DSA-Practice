    "def power(x,n):\n",
    "    ans = 1\n",
    "    while n>0:\n",
    "        if n%2==1:\n",
    "            ans*=x\n",
    "            n-=1\n",
    "\n",
    "        x*=x\n",
    "        n//=2\n",
    "\n",
    "    return ans\n",
    "\n",
    "print(power(3,5))\n",
    "\n",
    "    "
   ]
  }
